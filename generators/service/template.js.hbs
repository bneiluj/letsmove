(function (){
  "use strinct";

  // You should add {{#camelize}}{{name}}Service{{/camelize}} to the module dependencies to get the service {{name}} visible
  var MODULE_NAME = "{{#camelize}}{{name}}Service{{/camelize}}",
      SERVICE_NAME = "{{name}}";

  angular
  .module(MODULE_NAME, [])
  .factory(SERVICE_NAME, function() {
    return {
      module_name: function () {
        return MODULE_NAME;
      },
      service_name: SERVICE_NAME
    };
  });

})();

